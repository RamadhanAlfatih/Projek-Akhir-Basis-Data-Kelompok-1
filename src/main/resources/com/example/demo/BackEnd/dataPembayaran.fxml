<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.TreeTableColumn?>
<?import javafx.scene.control.TreeTableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<HBox style="-fx-alignment: center;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.demo.DataPembayaran">
   <children>
      <VBox style="-fx-alignment: center;">
         <children>
            <AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: D9BCBC;">
               <children>
                  <Text layoutX="16.0" layoutY="34.0" strokeType="OUTSIDE" strokeWidth="0.0" text="No. Pembayaran">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <TextField layoutX="190.0" layoutY="14.0" prefHeight="26.0" prefWidth="380.0" />
                  <Text layoutX="14.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Jenis Pembayaran">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <Text layoutX="16.0" layoutY="107.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Total Harga">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Text>
                  <TextField layoutX="190.0" layoutY="48.0" prefHeight="26.0" prefWidth="380.0" />
                  <TextField layoutX="190.0" layoutY="87.0" prefHeight="26.0" prefWidth="380.0" />
                  <Button fx:id="cari" layoutX="25.0" layoutY="128.0" mnemonicParsing="false" onAction="#clickCari" prefHeight="26.0" prefWidth="74.0" text="Cari" />
                  <Button fx:id="tambah" layoutX="139.0" layoutY="128.0" mnemonicParsing="false" onAction="#clickTambah" prefHeight="26.0" prefWidth="74.0" text="Tambah" />
                  <Button fx:id="edit" layoutX="262.0" layoutY="128.0" mnemonicParsing="false" onAction="#clickEdit" prefHeight="26.0" prefWidth="74.0" text="Edit" />
                  <Button fx:id="hapus" layoutX="380.0" layoutY="128.0" mnemonicParsing="false" onAction="#clickHapus" prefHeight="26.0" prefWidth="74.0" text="Hapus" />
                  <Button fx:id="kembali" layoutX="495.0" layoutY="128.0" mnemonicParsing="false" onAction="#clickKembali" prefHeight="26.0" prefWidth="74.0" text="Kembali" />
                  <TreeTableView layoutX="18.0" layoutY="166.0" prefHeight="229.0" prefWidth="563.0">
                     <columns>
                        <TreeTableColumn prefWidth="75.0" text="No. Pembayaran" />
                        <TreeTableColumn prefWidth="75.0" text="Jenis Pembayaran" />
                        <TreeTableColumn prefWidth="75.0" text="Total Harga" />
                     </columns>
                     <columnResizePolicy>
                        <TreeTableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                     </columnResizePolicy>
                  </TreeTableView>
               </children>
            </AnchorPane>
         </children>
      </VBox>
   </children>
</HBox>
